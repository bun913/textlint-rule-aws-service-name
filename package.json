{
  "version": "1.0.0",
  "release": {
    "branches": [
      "main",
      "next"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/changelog"
    ]
  },
  "commitlint": {
    "extentds": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "textlintrule"
  ],
  "files": [
    "lib/",
    "!lib/src",
    "!lib/regularRule",
    "!lib/test",
    "!lib/createRule*",
    "dict/",
    "build/"
  ],
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "testLint": "textlint-scripts test --exclude test/src/**/*",
    "createRule": "npx node lib/createRule.js",
    "prepublish": "yarn run --if-present build",
    "build": "textlint-scripts build",
    "prepare": "yarn build",
    "watch": "textlint-scripts build --watch"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.4",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@textlint/types": "^13.3.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "semantic-release": "^20.1.1",
    "textlint-tester": "^13.3.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "name": "textlint-rule-aws-service-name",
  "description": "Rule for AWS Product Names",
  "directories": {
    "test": "test"
  },
  "author": "bun913",
  "license": "MIT",
  "homepage": "https://github.com/bun913/textlint-rule-aws-service-name",
  "repository": {
    "type": "git",
    "url": "https://github.com/bun913/textlint-rule-aws-service-name.git"
  },
  "dependencies": {
    "textlint-rule-prh": "^5.3.0",
    "textlint-scripts": "^13.3.0",
    "yaml": "^2.2.1"
  }
}
